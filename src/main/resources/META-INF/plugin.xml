<idea-plugin>
    <id>co.anbora.labs.ngrok</id>
    <name>Ngrok</name>
    <vendor email="anboralabs@gmail.com" url="https://labs.anbora.co">Anbora-labs</vendor>

    <!-- please see https://plugins.jetbrains.com/docs/intellij/plugin-compatibility.html
         on how to target different products -->
    <depends>com.intellij.modules.platform</depends>

    <extensions defaultExtensionNs="com.intellij">
        <applicationService serviceImplementation="co.anbora.labs.ngrok.settings.NgrokSettings"/>
        <statusBarWidgetFactory id="ngrokWidget" implementation="co.anbora.labs.ngrok.ui.NgrokStatusBarWidgetFactory" order="before inspectionProfileWidget" />
    </extensions>

    <actions>
        <!-- Add your actions here -->
    </actions>
</idea-plugin>